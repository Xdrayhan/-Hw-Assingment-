#include<stdio.h>
#include<math.h>
//#define pi 3.1416

void main()
{
    float x,b,c;
    double A,B,C,result1,res2,res3;
    printf("enter three length of the slide: ");
    scanf("%f%f%f",&x,&b,&c);
    A=((b*b+c*c-x*x)/(2*b*c));
    result1=acos(A)*180/3.1416;
    B=((c*c+x*x-b*b)/(2*x*c));
    res2=acos(B)*180/3.1416;
    C=((x*x+b*b-c*c)/(2*x*b));
    res3=acos(C)*180/3.1416;
    printf("the angle of A is:%lf\n",result1);
    printf("the angle of B is:%lf\n",res2);
    printf("the angle of C is:%lf\n",res3);





}
